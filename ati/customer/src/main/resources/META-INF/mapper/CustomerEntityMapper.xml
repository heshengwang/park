    <!-- 渠道报表 -->
    <select id="selectDitchCount" parameterType="java.util.Map" resultType="com.kfwy.park.ati.customer.entity.CustomerEntity">
        SELECT
        a.ditch,
        COALESCE ( b.count, 0 ) ditchCount,
        COALESCE ( c.count, 0 ) reWatchCount
        FROM
        ( SELECT ditch FROM t_yq_ati_cus_customer
        <where>
            <if test="cpyCode != null">
                AND cpy_code = #{cpyCode}
            </if>
        </where>
        GROUP BY ditch ORDER BY ditch ASC ) a
        LEFT JOIN ( SELECT ditch, COUNT( ID ) FROM t_yq_ati_cus_customer
        <include refid="selectSql"/>
        GROUP BY ditch) b
        ON a.ditch = b.ditch
        LEFT JOIN (select cus_channel,count(id)   from t_yq_ati_cus_followup
        <where>
            <if test="createTimeStart != null">
                AND create_time <![CDATA[ >= ]]>  #{createTimeStart}
            </if>
            <if test="createTimeEnd != null">
                AND create_time <![CDATA[ <= ]]>  #{createTimeEnd}
            </if>
