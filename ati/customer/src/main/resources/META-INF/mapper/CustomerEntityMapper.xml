                AND followup_type = #{followupType}
            </if>
            <if test="cpyCode != null">
                AND cpy_code = #{cpyCode}
            </if>
        </where>
        GROUP BY cus_channel ) c
        on a.ditch = c.cus_channel
        ORDER BY a.ditch
    </select>

