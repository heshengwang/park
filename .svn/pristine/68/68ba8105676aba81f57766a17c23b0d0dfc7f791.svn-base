package com.kfwy.park.controller.sys.user.vo;

import com.gniuu.framework.service.AbstractServiceRequest;
import com.kfwy.park.sys.user.entity.UserEntity;

import java.util.List;

/**
 * Created by davidcun on 2018/5/18.
 */
public class UserServiceRequest extends AbstractServiceRequest<UserEntity> {

    private List<String> roleCodes;

    /**
     * 多账号登陆使用
     */
    private String keyword;

    /**
     * 新密码
     */
    private String newPwd;

    public List<String> getRoleCodes() {
        return roleCodes;
    }

    public void setRoleCodes(List<String> roleCodes) {
        this.roleCodes = roleCodes;
    }

    public String getKeyword() {
        return keyword;
    }

    public void setKeyword(String keyword) {
        this.keyword = keyword;
    }

    public String getNewPwd() {
        return newPwd;
    }

    public void setNewPwd(String newPwd) {
        this.newPwd = newPwd;
    }
}
