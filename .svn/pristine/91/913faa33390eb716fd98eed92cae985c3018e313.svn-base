package com.kfwy.park.task.attract.entity;

import com.gniuu.framework.entity.BaseEntity;

import java.util.Date;
import java.util.List;

/**
 * @Description 招商人员每日任务实体
 * @Auth xiongzhan
 * @Date 2018/8/2 14:33
 * @Version 1.0
 * @Param
 * @return
 */
public class TaskEntity extends BaseEntity {
    /**
     * 任务编号
     */
    private String taskCode;

    /**
     * 人员编号
     */
    private String empCode;

    /**
     * 姓名
     */
    private String empName;

    /**
     * 任务时间
     */
    private Date taskTime;

    /**
     * 任务时间段(只是查询用,不记入数据库)
     */
    private List<Date> taskTimeList;

    /**
     * 横幅
     */
    private Integer banner;

    /**
     * 宣传单
     */
    private Integer leaflets;

    /**
     * 微信朋友圈
     */
    private Integer friendsShare;

    /**
     * 任务描述
     */
    private String description;

    /**
     * 负责园区code
     */
    private String pkCode;

    /**
     * 负责园区
     */
    private String pkName;

    /**
     * 电话拜访 -
     * 暂时不用 2018-10-18
     */
    private Integer phoneVisits;

    /**
     * 客户来访：含[电话拜访]和[客户来访]
     */
    private Integer customerVisits;

    /**
     * 客户跟进
     */
    private Integer customerFollowup;

    /**
     * 主动获客
     */
    private Integer initiative;

    /**
     * 中介带看
     */
    private Integer agent;

    /**
     * 库房无忧
     */
    private Integer kuFangWuYou;

    /*---------------------------------------------get set方法---------------------------------------------*/

    public String getTaskCode() {
        return taskCode;
    }

    public void setTaskCode(String taskCode) {
        this.taskCode = taskCode;
    }

    public String getEmpCode() {
        return empCode;
    }

    public void setEmpCode(String empCode) {
        this.empCode = empCode;
    }

    public String getEmpName() {
        return empName;
    }

    public void setEmpName(String empName) {
        this.empName = empName;
    }

    public Date getTaskTime() {
        return taskTime;
    }

    public void setTaskTime(Date taskTime) {
        this.taskTime = taskTime;
    }

    public List<Date> getTaskTimeList() {
        return taskTimeList;
    }

    public void setTaskTimeList(List<Date> taskTimeList) {
        this.taskTimeList = taskTimeList;
    }

    public Integer getBanner() {
        return banner;
    }

    public void setBanner(Integer banner) {
        this.banner = banner;
    }

    public Integer getLeaflets() {
        return leaflets;
    }

    public void setLeaflets(Integer leaflets) {
        this.leaflets = leaflets;
    }

    public Integer getFriendsShare() {
        return friendsShare;
    }

    public void setFriendsShare(Integer friendsShare) {
        this.friendsShare = friendsShare;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public String getPkCode() {
        return pkCode;
    }

    public void setPkCode(String pkCode) {
        this.pkCode = pkCode;
    }

    public String getPkName() {
        return pkName;
    }

    public void setPkName(String pkName) {
        this.pkName = pkName;
    }

    public Integer getPhoneVisits() {
        return phoneVisits;
    }

    public void setPhoneVisits(Integer phoneVisits) {
        this.phoneVisits = phoneVisits;
    }

    public Integer getInitiative() {
        return initiative;
    }

    public void setInitiative(Integer initiative) {
        this.initiative = initiative;
    }

    public Integer getAgent() {
        return agent;
    }

    public void setAgent(Integer agent) {
        this.agent = agent;
    }

    public Integer getKuFangWuYou() {
        return kuFangWuYou;
    }

    public void setKuFangWuYou(Integer kuFangWuYou) {
        this.kuFangWuYou = kuFangWuYou;
    }

    public Integer getCustomerVisits() {
        return customerVisits;
    }

    public void setCustomerVisits(Integer customerVisits) {
        this.customerVisits = customerVisits;
    }

    public Integer getCustomerFollowup() {
        return customerFollowup;
    }

    public void setCustomerFollowup(Integer customerFollowup) {
        this.customerFollowup = customerFollowup;
    }
}
